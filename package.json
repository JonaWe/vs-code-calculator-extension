{
  "name": "calculator",
  "displayName": "Calculator",
  "description": "a simple calculator inside vs code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:calculator.evaluate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "calculator.evaluate",
        "title": "Evaluate"
      }
    ],
    "configuration": {
      "title": "Calculator",
      "properties": {
        "calculator.saveToClipboard": {
          "type": "boolean",
          "default": true,
          "description": "Specifies whether the result of a calculation is saved to the clipboard."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "mathjs": "^9.4.4"
  }
}
